{
  "name": "concierge-portal",
  "description": "The companion web app to the IBM FISE Concierge allowing friends of (elderly) users to set up accounts and manage the details of all users in their circle.",
  "author": "Ernest Badu",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://fise-concierge.web.app",
  "scripts": {
    "backend": "cd ../Backend; python3 tools/run.py",
    "browser": "open http://localhost:8081 & serve -s dist -p 8081 --no-clipboard",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "coverage": "start-server-and-test backend http://localhost:8100 jest --coverage; open coverage/lcov-report/index.html",
    "coverage:noserver": "jest --coverage --silent --maxWorkers -1; open coverage/lcov-report/index.html",
    "deploy": "set -e; npm run-script build; firebase deploy",
    "start": "set -e; npm run-script build; npm run-script browser",
    "start:backend": "npm run-script backend & npm start",
    "start:dev": "set -e; npm run-script build:dev; npm run-script browser",
    "start:dev:backend": "npm run-script backend & npm run-script start:dev",
    "test": "start-server-and-test backend http://localhost:8100 jest",
    "test:noserver": "jest --silent --maxWorkers -1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "axios": "^0.21.1",
    "firebase-storage-lite": "^0.3.0",
    "jest-fetch-mock": "^3.0.3",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.0",
    "vue-router": "^4.0.0-rc.1",
    "webpack-merge": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@vue/compiler-sfc": "3.0.2",
    "@vue/reactivity": "3.0.2",
    "@vue/runtime-core": "3.0.2",
    "@vue/test-utils": "^2.0.0-rc.1",
    "@webpack-cli/serve": "^1.1.0",
    "autoprefixer": "^10.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "robotstxt-webpack-plugin": "^7.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue": "3.0.2",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-loader": "^16.0.0-alpha.0",
    "vuex": "^4.0.0-beta.1",
    "vuex-persistedstate": "^4.0.0-beta.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
